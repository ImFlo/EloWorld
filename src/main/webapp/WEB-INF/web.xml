<!DOCTYPE web-app PUBLIC
          "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
          "http://java.sun.com/dtd/web-app_2_3.dtd" >
<!-- suivre le meme schema pour chaque servlet !-->
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>loging</servlet-name>
    <servlet-class>
      servlet.Loging
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loging</servlet-name>
    <url-pattern>/loging</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>initialize</servlet-name>
    <servlet-class>
      servlet.Initialize
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>initialize</servlet-name>
    <url-pattern>/initialize</url-pattern>
  </servlet-mapping>
</web-app>
